{
  "name": "SmartShopping Demo",
  "short_name": "Demo",
  "version": "1.0.0",
  "description": "Coupon Autoapply",
  "manifest_version": 2,
  "permissions": ["storage", "tabs"],
  "background": {
    "scripts": ["background-bundle.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": ["content-bundle.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "browser_action": {
    "default_icon": "smartshoppingIcon.png"
  },
  "icons": {
    "16": "smartshoppingIcon.png",
    "48": "smartshoppingIcon.png",
    "128": "smartshoppingIcon.png"
  }
}
